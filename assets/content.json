                    "audio": "m3-slide12.mp3",
                    "title": {
                        "type": "text-singleline",
                        "text": "A connected system",
                        "label": "Page Title"
                    },
                    "para": {
                        "type": "multipleline",
                        "text": "CNS helps us deliver the most efficient and effective nutrition by connecting each part of a pork system from beginning to end.",
                        "label": "paragraph"
                    },
                    "para1": {
                        "type": "multipleline",
                        "text": "",
                        "label": "paragraph"
                    },
                    "image": {
